sub palette-heat z
local r = 0
local g = 0
local b = 0
g = z
r = 3*z
if r > 1 then
r = 1
end if
b = 3*z-2
if b < 0 then
b = 0
end if
return rgb(r,g,b)
end sub

sub palette-paul1 z
local r = 0
local g = 0
local b = 0
local pi = acos(-1)
r = 0.5*(1-cos(pi*z))
g = 0.5*(1-cos(2*pi*z))
b = 0.5*(1+cos(pi*z))
return rgb(r,g,b)
end sub

sub palette-paul2 z
local r = 0
local g = 0
local b = 0
local pi = acos(-1)
r = 0.5*(1-cos(pi*z))
g = 0
b = 0.5*(1+cos(pi*z))
return rgb(r,g,b)
end sub

