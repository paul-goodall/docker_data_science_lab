sub palette-backgr z
local r = 0
local g = 0
local b = 0

r = 4*z
g = 4*z
b = 4*z
if z > 0.25 then
r = 0
g = 4*z-1
b = 2-4*z
end if
if z > 0.5 then
r = 4*z-2
g = 1
b = 0
end if
if z > 0.75 then
r = 1
g = 4*(1-z)
b = 0
end if
return rgb(r,g,b)
end sub

